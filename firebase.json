{
  "hosting": {
    "public": "responses",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Content-type",
            "value": "application/json"
          }
        ]
      }
    ]
  }
}